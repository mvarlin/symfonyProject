{% extends 'base.html.twig' %}
 
{% block title %}lane{% endblock %}
 
{% block body %}
    <div class="container">
        <h2 class="text-center mt-5 mb-3">Show lane</h2>
        <div class="card">
            <div class="card-header">
                <a class="btn btn-outline-info float-right" href="{{ path('app_lane_index') }}"> 
                    View All lanes
                </a>
            </div>
            <div class="card-body">
                <b class="text-muted">Lane:</b>
                <p>{{ lane.lanename }}</p>
                <b class="text-muted">Description:</b>
                <p>{{ lane.lanedescription }}</p>
                {# <b class="text-muted">Date Created:</b>
                <p>{{ lane.createdAt ? lane.createdAt|date('Y-m-d H:i:s') : '' }}</p>
                <b class="text-muted">Date Updated:</b>
                <p>{{ lane.updatedAt ? lane.updatedAt|date('Y-m-d H:i:s') : '' }}</p> #}
            </div>
        </div>
    </div>
{% endblock %}